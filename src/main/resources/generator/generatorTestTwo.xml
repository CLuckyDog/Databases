<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<!-- 配置详解 https://blog.csdn.net/zsq520520/article/details/50952830 -->
<generatorConfiguration>
	<classPathEntry
			location="D:\Maven-Repository\mysql\mysql-connector-java\5.1.36\mysql-connector-java-5.1.36.jar"/>

	<context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true"/>
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://127.0.0.1:3306/testOne?generateSimpleParameterMetadata=true"
						userId="root" password="Hope_run123">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false"/>
		</javaTypeResolver>

		<!-- generate Model 用于生成实体类-->
		<javaModelGenerator targetPackage="com.databases.databases.domain.two" targetProject="C:\Users\pan_zhongjian\Desktop\mybatis">
			<property name="enableSubPackages" value="false"/>
			<property name="trimStrings" value="true"/>
		</javaModelGenerator>

		<!-- generate xml 生成mapper.xml文件-->
		<sqlMapGenerator targetPackage="mapper.two" targetProject="C:\Users\pan_zhongjian\Desktop\mybatis">
			<property name="enableSubPackages" value="false"/>
		</sqlMapGenerator>

		<!-- generate Mapper 生成dao接口文件-->
		<javaClientGenerator type="XMLMAPPER"
							 targetPackage="com.databases.databases.dao.two"
							 targetProject="C:\Users\pan_zhongjian\Desktop\mybatis" implementationPackage="">
			<property name="enableSubPackages" value="false"/>
			<property name="exampleMethodVisibility" value="public"/>
		</javaClientGenerator>

		<!-- 运行完添加注释 指定哪些表生成-->
		<!--<table tableName="kfc_discard_detail" domainObjectName="DiscardDetail" enableCountByExample="false" enableUpdateByExample="false"
                    enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <property name="useActualColumnNames" value="true"/>
        </table>-->

		<table tableName="t_news" domainObjectName="TNews" enableCountByExample="false" enableUpdateByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
			<property name="useActualColumnNames" value="true"/>
		</table>

		<table tableName="t_users" domainObjectName="TUsers" enableCountByExample="false" enableUpdateByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
			<property name="useActualColumnNames" value="true"/>
		</table>
	</context>
</generatorConfiguration>
